    "husky": "^7.0.0",
    "lint-staged": ">=12.1.2",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^3.0.1",
    "tailwindcss-debug-screens": "^2.1.0"
